{
    "name": "paws-frontend-showcase",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node app.js",
        "watch": "concurrently \"nodemon app.js\" \"browser-sync start --proxy 'localhost:3000' --files 'views/**/*.pug, public/**/*.*, app.js' --port 4000\""
    },
    "dependencies": {
        "express": "5.2.1",
        "pug": "3.0.3",
        "serve-favicon": "2.5.1"
    },
    "devDependencies": {
        "browser-sync": "3.0.4",
        "concurrently": "9.2.1",
        "nodemon": "3.1.11"
    }
}
